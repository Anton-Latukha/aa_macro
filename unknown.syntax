context default green blue

    define kcolor white
	define bcolor white
	define barcolor white
	define commacolor white
	define htmlcolor white

	define squigglycolor brightcyan
	define stylecolor red

	define bracecolor brightmagenta
	define builtincolor yellow

	keyword , commacolor
	keyword | barcolor
	keyword [b] yellow red
	keyword whole co builtincolor
	keyword whole sp builtincolor
	keyword whole lb builtincolor
	keyword whole rb builtincolor
	keyword whole ls builtincolor
	keyword whole rs builtincolor
	keyword whole nl builtincolor
	keyword whole lf builtincolor
	keyword [parm yellow red
	keyword >= green
	keyword <= green
	keyword [ bracecolor
	keyword ] bracecolor
	keyword { squigglycolor
	keyword } squigglycolor

	context < > htmlcolor
	keyword { squigglycolor
	keyword } squigglycolor
	keyword [ bracecolor
	keyword ] bracecolor

	context exclusive { \s stylecolor
	keyword { squigglycolor
	keyword } squigglycolor
	keyword [ bracecolor
	keyword ] bracecolor

	context exclusive [ \s builtincolor
	keyword { squigglycolor
	keyword } squigglycolor
	keyword [ bracecolor
	keyword ] bracecolor

	context exclusive { } green
	keyword [ bracecolor
	keyword ] bracecolor
	keyword { squigglycolor
	keyword } squigglycolor
